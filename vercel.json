{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "^/detect-di-word$",
        "dest": "app.py"
      },
      {
        "src": "^/detect",
        "dest": "app.py"
      },
      {
        "src": "^/(.*)",
        "dest": "app.py"
      }
    ]
  }
  